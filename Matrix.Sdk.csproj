<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <LangVersion>9</LangVersion>

        <PackageId>Matrix.Sdk</PackageId>
        <PackageTags>matrix sdk p2p chat room</PackageTags>
        
        <Authors></Authors>
        <Product>Matrix.Sdk</Product>
